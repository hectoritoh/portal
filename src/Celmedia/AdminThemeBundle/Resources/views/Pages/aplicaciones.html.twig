{% extends "CelmediaAdminThemeBundle::template.html.twig" %}

{% block content %}



<div class="widget stacked widget-table action-table">

	<div class="widget-header">
		<i class="icon-th-list"></i>
		<h3>Aplicaciones</h3>
	</div> <!-- /widget-header -->

	<div class="widget-content">

		<table class="table table-striped table-bordered">
			<thead>
				<tr>
					<th>Codigo</th>
					<th>Nombre</th>
					<th>Token</th>
					<th>Fecha registro</th>
					<th class="td-actions"></th>
				</tr>
			</thead>
			<tbody>

			{% for aplicacion in aplicaciones %}
    
				<tr>
					<td>{{ aplicacion.id }}</td>
					<td>{{ aplicacion.nombre }}</td>
					<td>{{  aplicacion.token }}</td>
					<td>{{  aplicacion.created | date('Y-m-d')  }}</td>
						<td class="td-actions">
							<a href="javascript:;" class="btn btn-xs btn-primary">
								<i class="btn-icon-only icon-ok"></i>										
							</a>
						</td>
					</tr>

			{% endfor %}


				</tbody>
			</table>

		</div> <!-- /widget-content -->

	</div> <!-- /widget -->




	{% endblock %}