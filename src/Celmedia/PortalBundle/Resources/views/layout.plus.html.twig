
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title>{% block title %}CRM interactivo{% endblock %}</title>
  <!-- Always force latest IE rendering engine (even in intranet) & Chrome Frame -->
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" >
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="">
  <meta name="author" content="">

  <link href='http://fonts.googleapis.com/css?family=Roboto:400,300,700' rel='stylesheet' type='text/css'>
  {% block estilos %}
  {% stylesheets 
  '@CelmediaPortalBundle/Resources/public/bootplus/css/bootplus.css'
  '@CelmediaPortalBundle/Resources/public/bootplus/css/bootplus-responsive.css' 
  '@CelmediaPortalBundle/Resources/public/font-awesome/css/font-awesome.css' 

  '@CelmediaPortalBundle/Resources/public/css/reset.css'
  '@CelmediaPortalBundle/Resources/public/css/style.plus.css'
  '@CelmediaPortalBundle/Resources/public/css/style.css'
  filter='cssrewrite' %}
  <link rel="stylesheet" href="{{ asset_url }}" />
  {% endstylesheets %} 
  {% endblock %}

  <link rel="apple-touch-icon-precomposed" sizes="144x144" href="../assets/ico/apple-touch-icon-144-precomposed.png">
  <link rel="apple-touch-icon-precomposed" sizes="114x114" href="../assets/ico/apple-touch-icon-114-precomposed.png">
  <link rel="apple-touch-icon-precomposed" sizes="72x72" href="../assets/ico/apple-touch-icon-72-precomposed.png">
  <link rel="apple-touch-icon-precomposed" href="../assets/ico/apple-touch-icon-57-precomposed.png">
  <link rel="shortcut icon" href="../assets/ico/favicon.png">
</head>

<body  data-spy="scroll" data-target=".bs-docs-sidebar"    {% block bodyStyle %}{% endblock %} >


  {% block menu %}


<!-- Navbar
  ================================================== -->
  <div class="navbar navbar-fixed-top">
    <div class="navbar-inner">
      <div class="container">
        <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="brand" href="{{  path('portal_home') }}">Portal</a>
        <div class="nav-collapse collapse">
          <ul class="nav">

            <li class="{% if app.request.get('_route') == 'portal_home' %}active{% endif %}">
              <a href="{{  path('portal_home') }}">Home</a>
            </li>
            <li class="{% if app.request.get('_route') == 'portal_setup' %}active{% endif %}">
              <a href="{{  path('portal_setup') }}">Enviar Campana</a>
            </li>
            <li class="{% if app.request.get('_route') == 'clientes' %}active{% endif %}">
              <a href="{{  path('clientes') }}">Clientes</a>
            </li>
            <li class="{% if app.request.get('_route') == 'campanas' %}active{% endif %}">
              <a href="{{  path('campanas') }}">Campa√±as</a>
            </li>
            <li class="{% if app.request.get('_route') == 'reportes' %}active{% endif %}">
              <a href="{{  path('reportes') }}">Reportes</a>
            </li>
           
          </ul>
        </div>
      </div>
    </div>
  </div>
  {% endblock %}



  <div class="container"  style="margin-top: 60px;" >


    {% block contenido %}

    {% endblock %}




  </div> 

  {%  block js %}
  {% javascripts
  '@CelmediaPortalBundle/Resources/public/js/jquery-1.10.2.js' 
  '@CelmediaPortalBundle/Resources/public/js/bootstrap.js' 
  '@CelmediaPortalBundle/Resources/public/js/jquery-ui.min.js'    
  '@CelmediaPortalBundle/Resources/public/js/jquery.blockUI.js'
  '@CelmediaPortalBundle/Resources/public/js/jquery.form.js'
  '@CelmediaPortalBundle/Resources/public/js/script.js'     
  %}
  <script  type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}
  {% endblock %}



</body>
</html>
